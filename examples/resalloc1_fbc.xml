      <body xmlns="http://www.w3.org/1999/xhtml">
	  <p>This model uses the deFBA standard 1.0</p>
      </body>
    </notes>
    
    <listOfCompartments>
      <compartment sboTerm="SBO:0000410" id="default_compartment" spatialDimensions="3" size="1" constant="true"/>
      <compartment id="external"    	 />
      <compartment id="metabolites" 	 />
      <compartment id="bio" />
      <compartment id="storage" />
    </listOfCompartments>
    
    <listOfSpecies>
      <species id="N1"  compartment="external" initialAmount="10"  constant="false"/>
      <species id="N2"  compartment="external" initialAmount="10"  constant="false"/>
      <species id="O2"  compartment="external" constant="true" boundaryCondition="true"/>
      <species id="N"   compartment="metabolites" initialAmount="0"  constant="true"/>
      <species id="ATP" compartment="metabolites" initialAmount="0"  constant="true"/>
      <species id="AA"  compartment="metabolites" initialAmount="0"  constant="true"/>
      <species id="Etrans1" name="Transporter enzyme" compartment="bio" initialAmount="0.005555555555555556"  constant="false"/>
      <species id="Etrans2" name="Cofactor of transporter enzyme" compartment="bio" initialAmount="0.0"  constant="false"/>
      <species id="Emetab1" name="Generic metabolic enzyme" compartment="bio" initialAmount="1.9442790116926745"  constant="false"/>
      <species id="Emetab2" name="Specialized metabolic enzyme" compartment="bio" initialAmount="0.008000000399798693"  constant="false"/>
      <species id="R" name="Ribosome" compartment="bio" initialAmount="0.033641975308641964"  constant="false"/>
      <species id="S" name="Structrual biomass component" compartment="bio" initialAmount="0.7499999999999999"  constant="false"/>
      <species id="Complex_1" name="Transporter complex" compartment="bio" initialAmount="0.004166667083123639" constant="false"/>
      <species id="Stor" name="Storage molecules" compartment="storage" initialAmount="0.0" constant="false"/>
      <species id="Estor" name="Metabolic enzyme for handling storage" compartment="bio" initialAmount="0.0" constant="false" />
<!--       <species id="Estor_b" name="Metabolic enzyme for depletion storage" compartment="bio" initialAmount="0.0" constant="false" /> -->
