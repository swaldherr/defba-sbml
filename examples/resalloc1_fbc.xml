    <fbc:listOfGeneProducts>
      <fbc:geneProduct fbc:id="Etrans1"   label="1*GTRANS1"     associatedSpecies="Etrans1"/>
      <fbc:geneProduct fbc:id="Etrans2"   label="1*GTRANS2"     associatedSpecies="Etrans2"/>
      <fbc:geneProduct fbc:id="Complex_1" label="1*GTRANS1 AND 1*GTRANS2"   associatedSpecies="Complex_1" />
      <fbc:geneProduct fbc:id="Emetab1"   label="1*GMETAB1"     associatedSpecies="Emetab1"/>
      <fbc:geneProduct fbc:id="Emetab2"   label="1*GMETAB2"     associatedSpecies="Emetab2"/>
      <fbc:geneProduct fbc:id="R"         label="1*GRIBOS"      associatedSpecies="R"/>
      <fbc:geneProduct fbc:id="Estor"     label="1*GSTORF"	associatedSpecies="Estor"/>
    </fbc:listOfGeneProducts>
    <listOfReactions>
      <reaction id="Trans1" reversible="true" fast="false" fbc:lowerFluxBound="kcatt" fbc:upperFluxBound="kcatt">
	<fbc:geneProductAssociation fbc:id="Etrans1">
	  <fbc:geneProductRef fbc:geneProduct="Etrans1" />
	</fbc:geneProductAssociation>
        <listOfReactants>
          <speciesReference species="N1" stoichiometry="1" constant="true"/>
	  <speciesReference species="O2" stoichiometry="2" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="N" stoichiometry="1" constant="true"/>
        </listOfProducts>
      </reaction>
      <reaction id="Trans2" reversible="true" fast="false" fbc:lowerFluxBound="kcatC" fbc:upperFluxBound="kcatC">
        <fbc:geneProductAssociation fbc:id="Complex_1">
          <fbc:geneProductRef fbc:geneProduct="Complex_1"/>
        </fbc:geneProductAssociation>
        <listOfReactants>
          <speciesReference species="N2" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="N" stoichiometry="1" constant="true"/>
        </listOfProducts>
      </reaction>-->
      <reaction id="Metab1_1" reversible="false" fast="true" fbc:lowerFluxBound="kcat1" fbc:upperFluxBound="zero">
