        </listOfReactants>
        <listOfProducts>
          <speciesReference species="N" stoichiometry="1" constant="true"/>
        </listOfProducts>
      </reaction>
      <reaction id="Trans2" reversible="true" fast="false" fbc:lowerFluxBound="kcatt" fbc:upperFluxBound="kcatt">
