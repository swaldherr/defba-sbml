    <listOfParameters>
      <parameter constant="true" id="kcatt"  value="1800"   />
      <parameter constant="true" id="kcatC"  value="2400"   />
      <parameter constant="true" id="kcat1"  value="2000"   />
      <parameter constant="true" id="kcat2"  value="2500"   />
      <parameter constant="true" id="zero"   value="0.0"    />
      <parameter constant="true" id="kcatR1" value="10"     />
      <parameter constant="true" id="kcatR2" value="16.67"  />
      <parameter constant="true" id="kcatR3" value="5"      />
      <parameter constant="true" id="kcatR4" value="6.25"   />
      <parameter constant="true" id="kcatR5" value="1"      />
      <parameter constant="true" id="kcatR6" value="10"     />
      <parameter constant="true" id="kcatC1" value="6.25"   />
      <parameter constant="true" id="kcatSf" value="25"     />
      <parameter constant="true" id="kcatSb" value="30"     />
